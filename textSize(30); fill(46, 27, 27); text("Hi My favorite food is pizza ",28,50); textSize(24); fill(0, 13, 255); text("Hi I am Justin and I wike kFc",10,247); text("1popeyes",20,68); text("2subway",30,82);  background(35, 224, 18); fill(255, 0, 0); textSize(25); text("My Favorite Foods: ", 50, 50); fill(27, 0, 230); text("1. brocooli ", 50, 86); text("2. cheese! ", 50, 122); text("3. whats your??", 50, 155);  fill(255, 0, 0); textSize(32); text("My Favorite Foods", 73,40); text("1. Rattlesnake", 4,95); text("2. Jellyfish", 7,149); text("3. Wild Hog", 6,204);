var bodyX = 192;
var bodyY = 195;
var bodyW = 234;
var bodyH = bodyW/-2;
var aPos = 45;

draw= function() {
    aPos+=2;
    
     background(30, 54, 7);
    fill(99, 56, 10);
    ellipse(bodyX , bodyX, bodyY + 44, bodyW + -2); // face
    fill(18, 15, 15);
    ellipse(bodyX * 2/3, bodyY * 3/4, bodyW * 2/11, aPos); //left eye
    ellipse(bodyX * -44/-32, bodyY * 3/4, bodyW * 2/11, aPos); //right eye
     ellipse(bodyX * -33/-32, bodyY + 20, bodyW * 2/4, 45);  //nose
     fill(143, 132, 132);
     ellipse(bodyX - 20, bodyY + 21, bodyW - 207, 22); //nostril
     ellipse(bodyX + 27, bodyY + 21, bodyW - 209, 22); //nostril
   strokeWeight(5);
     line(bodyX + 73,bodyY + -90,bodyX + 135, bodyY + -153); //antler
     line(bodyX + -73,bodyY + -88,bodyX + -123,bodyY + -151);  //antler
     line(bodyX + -143,bodyY + -114,bodyX + -92,bodyY + -111);    //antler
     line(bodyX + 149,bodyY + -105, bodyX + 107, bodyY + -125);  //antler
     triangle(bodyX + -61, bodyY + 46,bodyX + 57, bodyY + 71, bodyX + -21,bodyX + 105);
 
 
 
  
};
