  fill(252, 247, 151);
    ellipse(xPos, yPos, 10, 10);
ellipse(Xpos,Ypos,10,10);
ellipse(rpos, spos, 10,10);
ellipse(lpos,dpos,10,10);

fill(119, 168, 252);
ellipse(200, cpos, 15,15);
ellipse(150, zpos, 15,15);
